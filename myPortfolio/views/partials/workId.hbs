<h1>{{workId.title}}</h1>
<h5>Posted on {{formatDate workId.dateCreation}}</h5>

{{#each workId.tag}}
<p class="vignette-tag">{{this}}</p>
{{/each}}

<h3>Description</h3>
<p>{{workId.description}} </p>


<h3>Screenshots</h3>



<iframe width="560" height="315" src="{{workId.video}}" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
{{!-- <iframe width="500px" height="250px" src="{{workId.video}}" frameborder="0"
    allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe> --}}

<img style="width: 250px;" src="{{workId.imagePreview}}" alt="">
